{
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/v0.1/:path*(/?)",
      "destination": "https://langchain-v01.vercel.app/v0.1/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/docs",
      "destination": "/v0.2/docs/introduction"
    },
    {
      "source": "/",
      "destination": "/v0.2/docs/introduction"
    },
    {
      "source": "/docs/:path*(/?)",
      "destination": "/v0.1/docs/:path*"
    }
  ]
}
